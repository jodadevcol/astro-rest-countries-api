{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": [
				"./src/components/*"
			],
			"@layout/*": [
				"./src/layout/*"
			],
			"@services/*": [
				"./src/services/*"
			],
			"@store/*": [
				"./src/store/*"
			],
			"@tools/*": [
				"./src/tools/*"
			],
			"@types-local/*": [
				"./src/types/*"
			],
			"@icons/*": [
				"./src/icons/*"
			],
			"@const/*": [
				"./src/const/*"
			]
		}
	}
}